{
  "name": "react-multi-picker",
  "version": "0.5.1",
  "description": "a multi selector like antd-mobile Piker",
  "main": "build",
  "keywords": [
    "react",
    "mobile",
    "picker",
    "selector",
    "antd-mobile"
  ],
  "scripts": {
    "pre": "tsc webpack.config.ts --allowSyntheticDefaultImports --esModuleInterop || mv webpack.config.js .DoNotEditMe.js",
    "build": "npm run build:production",
    "build:develop": "npm run pre && cross-env NODE_ENV=production --config .DoNotEditMe.js",
    "build:staging": "npm run pre && cross-env NODE_ENV=production webpack  --config .DoNotEditMe.js",
    "build:production": "npm run pre && cross-env NODE_ENV=production webpack  --config .DoNotEditMe.js",
    "start": "npm run pre && cross-env NODE_ENV=development webpack-dev-server  --config .DoNotEditMe.js"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/ericyang89/multi-picker.git"
  },
  "author": "ericyang89",
  "license": "ISC",
  "dependencies": {
    "invariant": "^2.2.4"
  },
  "devDependencies": {
    "@types/html-webpack-plugin": "^3.2.0",
    "@babel/core": "^7.0.1",
    "@babel/plugin-proposal-class-properties": "^7.0.0",
    "@babel/plugin-proposal-decorators": "^7.0.0",
    "@babel/plugin-proposal-do-expressions": "^7.0.0",
    "@babel/plugin-proposal-export-default-from": "^7.0.0",
    "@babel/plugin-proposal-export-namespace-from": "^7.0.0",
    "@babel/plugin-proposal-function-bind": "^7.0.0",
    "@babel/plugin-proposal-function-sent": "^7.0.0",
    "@babel/plugin-proposal-json-strings": "^7.0.0",
    "@babel/plugin-proposal-logical-assignment-operators": "^7.0.0",
    "@babel/plugin-proposal-nullish-coalescing-operator": "^7.0.0",
    "@babel/plugin-proposal-numeric-separator": "^7.0.0",
    "@babel/plugin-proposal-optional-chaining": "^7.0.0",
    "@babel/plugin-proposal-pipeline-operator": "^7.0.0",
    "@babel/plugin-proposal-throw-expressions": "^7.0.0",
    "@babel/plugin-syntax-dynamic-import": "^7.0.0",
    "@babel/plugin-syntax-import-meta": "^7.0.0",
    "@babel/polyfill": "^7.0.0",
    "@babel/preset-env": "^7.0.0",
    "@babel/preset-react": "^7.0.0",
    "@babel/preset-typescript": "^7.1.0",
    "@types/invariant": "^2.2.29",
    "@types/node": "^10.12.0",
    "@types/react": "^16.4.18",
    "@types/react-dom": "^16.0.9",
    "@types/webpack": "^4.4.17",
    "@types/webpack-dev-server": "^3.1.1",
    "babel-loader": "^8.0.4",
    "babel-plugin-import": "^1.10.0",
    "babel-polyfill": "^6.26.0",
    "babel-preset-react-hmre": "^1.1.1",
    "clean-webpack-plugin": "^0.1.19",
    "cross-env": "^5.2.0",
    "css-loader": "^1.0.0",
    "file-loader": "^2.0.0",
    "fork-ts-checker-webpack-plugin": "^0.4.14",
    "html-webpack-plugin": "^3.2.0",
    "mini-css-extract-plugin": "^0.4.4",
    "react-hot-loader": "^4.3.12",
    "style-loader": "^0.23.1",
    "ts-loader": "^5.2.2",
    "ts-node": "^7.0.1",
    "tsconfig-paths": "^3.6.0",
    "typescript": "^3.1.3",
    "uglifyjs-webpack-plugin": "^2.0.1",
    "url-loader": "^1.1.2",
    "webpack": "^4.18.0",
    "webpack-cli": "^3.1.0",
    "webpack-dev-server": "^3.1.9"
  },
  "peerDependencies": {
    "antd-mobile": "^2.2.5",
    "react": "^16.5.2",
    "react-dom": "^16.5.2"
  }
}
